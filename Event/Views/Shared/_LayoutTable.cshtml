@using MyEventPlan.Data.Service.Enum
<!DOCTYPE html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link type="text/css" href="~/Content/Dashboard/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link type="text/css" href="~/Content/Dashboard/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet">
    <link type="text/css" href="~/Content/Dashboard/css/theme.css" rel="stylesheet">
    <link type="text/css" href="~/Content/Dashboard/images/icons/css/font-awesome.css" rel="stylesheet">
    <link type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600"
          rel="stylesheet">
    <link href="~/Content/PageLoader/css/jquery-loading.min.css" rel="stylesheet"/>
    @RenderSection("css", false)
</head>
<body id="page-top" class="index">
<div id="jquery-script-menu">
    <div class="jquery-script-center">
        <div class="jquery-script-ads">
            <script type="text/javascript">
<!--
                google_ad_client = "ca-pub-2783044520727903";
/* jQuery_demo */
                google_ad_slot = "2780937993";
                google_ad_width = 728;
                google_ad_height = 90;
//-->
            </script>
            <script type="text/javascript"
                    src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
                
            </script>
        </div>
        <div class="jquery-script-clear"></div>
    </div>
</div>
@{ Html.RenderPartial("_PartialNavigation"); }
<!-- /navbar -->
<div class="wrapper">
    <div class="container">
        <div class="row">
            <div class="span3">
                @{ Html.RenderPartial("_PartialSidebar"); }
                <!--/.sidebar-->
            </div>
            <div class="span9">
                <div class="content">
                    <div class="module">
                        <div class="module-head">
                            <h3>
                                @ViewBag.TableHeader

                            </h3>
                        </div>
                        <div class="module-body table">
                            @RenderSection("add", false)
                            @if (TempData["eventtype"] != null)
                            {
                                if (TempData["notificationtype"].Equals(NotificationType.Success.ToString()))
                                {
                                    <div class="alert-success alert-dismissable">@TempData["eventtype"]</div>
                                }
                                else if (TempData["notificationtype"].Equals(NotificationType.Error.ToString()))
                                {
                                    <div class="alert-danger alert-dismissable">@TempData["eventtype"]</div>
                                }
                                else if (TempData["notificationtype"].Equals(NotificationType.Info.ToString()))
                                {
                                    <div class="alert-info alert-dismissable">@TempData["eventtype"]</div>
                                }
                            }
                            @if (TempData["mapping"] != null)
                            {
                                if (TempData["notificationtype"].Equals(NotificationType.Success.ToString()))
                                {
                                    <div class="alert-success alert-dismissable">@TempData["mapping"]</div>
                                }
                                else if (TempData["notificationtype"].Equals(NotificationType.Error.ToString()))
                                {
                                    <div class="alert-danger alert-dismissable">@TempData["mapping"]</div>
                                }
                                else if (TempData["notificationtype"].Equals(NotificationType.Info.ToString()))
                                {
                                    <div class="alert-info alert-dismissable">@TempData["mapping"]</div>
                                }
                            }
                            @if (TempData["staff"] != null)
                            {
                                if (TempData["notificationtype"].Equals(NotificationType.Success.ToString()))
                                {
                                    <div class="alert-success alert-dismissable">@TempData["staff"]</div>
                                }
                                else if (TempData["notificationtype"].Equals(NotificationType.Error.ToString()))
                                {
                                    <div class="alert-danger alert-dismissable">@TempData["staff"]</div>
                                }
                                else if (TempData["notificationtype"].Equals(NotificationType.Info.ToString()))
                                {
                                    <div class="alert-info alert-dismissable">@TempData["staff"]</div>
                                }
                            }
                            @RenderBody()
                        </div>
                    </div>
                    <!--/.module-->
                </div>
                <!--/.content-->
            </div>
        </div>
    </div>
    <!--/.container-->
</div>
<!--/.wrapper-->
<div class="footer">
    <div class="container">
        <b class="copyright">@DateTime.Now.Year </b>All rights reserved.
    </div>
</div>
<script src="~/Content/Dashboard/scripts/jquery-1.9.1.min.js" type="text/javascript"></script>
<script src="~/Content/Dashboard/scripts/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
<script src="~/Content/Dashboard/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<script src="~/Content/Dashboard/scripts/flot/jquery.flot.js" type="text/javascript"></script>
<script src="~/Content/Dashboard/scripts/flot/jquery.flot.resize.js" type="text/javascript"></script>
<script src="~/Content/Dashboard/scripts/datatables/jquery.dataTables.js" type="text/javascript"></script>
<script src="~/Content/Dashboard/scripts/common.js" type="text/javascript"></script>
<script src="~/Content/PageLoader/js/jquery-loading.js"></script>
</body>
<script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-36251023-1']);
    _gaq.push(['_setDomainName', 'jqueryscript.net']);
    _gaq.push(['_trackPageview']);

    (function() {
        var ga = document.createElement('script');
        ga.type = 'text/javascript';
        ga.async = true;
        ga
            .src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') +
            '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ga, s);
    })();

</script>
@RenderSection("scripts", false)