@model Event.Data.Objects.Entities.VendorImage
@{
    Layout = "../../Shared/Layout/_LayoutModal.cshtml";
}
@using (Html.BeginForm("Edit", "VendorImages", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m=>m.VendorImageId)
    @Html.HiddenFor(m=>m.VendorId)
    @Html.HiddenFor(m=>m.ImageOne)
    @Html.HiddenFor(m=>m.ImageTwo)
    @Html.HiddenFor(m=>m.ImageThree)
    @Html.HiddenFor(m=>m.ImageFour)
    @Html.HiddenFor(m=>m.ImageFive)
    @Html.HiddenFor(m=>m.ImageSix)
    @Html.HiddenFor(m=>m.ImageSeven)
    @Html.HiddenFor(m=>m.ImageEight)
    @Html.HiddenFor(m=>m.ImageNine)
    @Html.HiddenFor(m=>m.ImageTen)
    @Html.HiddenFor(m=>m.CreatedBy)
    @Html.HiddenFor(m=>m.DateCreated)
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <div class="controls">
            <input type="file" name="imageThree" class="form-control input-md" />
            @Html.ValidationMessageFor(model => model.ImageThree, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-4">
            <input type="submit" value="Upload Image" class="btn btn-default" />
        </div>
    </div>
}
