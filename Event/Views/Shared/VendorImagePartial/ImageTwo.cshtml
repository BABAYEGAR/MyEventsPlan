@model Event.Data.Objects.Entities.VendorImage
@{
    Layout = "../../Shared/Layout/_LayoutModal.cshtml";
}
@using (Html.BeginForm("Create", "VendorImages", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="control-group">
        @Html.LabelFor(model => model.ImageTwo, new { @class = "control-label" })
        <div class="controls">
            <input type="file" name="imageTwo" />
            @Html.ValidationMessageFor(model => model.ImageTwo, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="control-group">
        <div class="controls">
            <input type="submit" value="Upload Image" class="btn btn-default" />
        </div>
    </div>
}
