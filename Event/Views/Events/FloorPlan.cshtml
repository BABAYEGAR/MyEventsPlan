@using Event.Data.Objects.Entities
@model Event
@{
    ViewBag.Title = "Calendar";
    ViewBag.Header = "Calendar";
    ViewBag.SubHeader = "Event Calendar";
    Layout = "../Shared/Layout/_LayoutCalendar.cshtml";

    var loggedinuser = Session["myeventplanloggedinuser"] as AppUser;
}
@section links{

    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="@Url.Action("Dashboard", "Home")">My Dashboard</a>
        </li>
    </ol>
}
<div class="panel-body">
    <div id="chart">
    </div>
</div>

@section scripts{
    <script type="text/javascript" src="https://cdn.seats.io/chart.js"></script>
    <script>
        var chart = new seatsio.SeatingChart({
            divId: "chart",
            publicKey: "publicDemoKey",
            event: "smallTheatreEvent2"
        }).render();
    </script>


}